{
  "name": "db-postgresql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch",
    "sql": "tsx scripts/sqlFileRunner.ts",
    "sql:en": "tsx scripts/sqlFileRunner.ts --en",
    "db:up": "docker compose -f docker/docker-compose.yaml -p pg17dev up -d --wait",
    "db:down": "docker compose -f docker/docker-compose.yaml -p pg17dev down",
    "db:reset": "docker compose -f docker/docker-compose.yaml -p pg17dev down -v"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.3.1",
    "@types/pg": "^8.15.5",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "pg": "^8.16.3",
    "prisma": "^6.16.3"
  }
}
